{
  "name": "wordl3-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "clean": "rm -rf .next .soroban .soroban-soropuzzles node_modules",
    "setup": "npm run clean && ./initialize.sh ${NETWORK:-testnet} && npm run build-contracts && npm install",
    "build-contracts": "soroban contract bindings typescript --wasm ./target/wasm32-unknown-unknown/release/soropuzzles_project.wasm --id $(cat ./.soroban/soropuzzles_id) --output-dir ./.soroban/soropuzzles-contract --network \"$(cat ./.soroban-soropuzzles/network)\" --overwrite"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@stellar/stellar-sdk": "^11.2.2",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/supabase-js": "^2.39.3",
    "@thirdweb-dev/auth": "^4.1.30",
    "@thirdweb-dev/react": "^4.4.5",
    "@thirdweb-dev/sdk": "^4.0.33",
    "@walletconnect/modal": "^2.6.2",
    "sorozpuzzles-contract": "file:.soroban/soropuzzles-contract",
    "ethers": "^5.7.2",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.0",
    "pino-pretty": "^10.3.1",
    "react": "^18",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-otp-input": "^3.1.1",
    "react-toastify": "^10.0.4",
    "sass": "^1.70.0",
    "use-sound": "^4.0.1",
    "viem": "^1.21.4",
    "wagmi": "^1.4.13",
    "web3": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "18.2.52",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "file-loader": "^6.2.0",
    "postcss": "^8",
    "scss": "^0.2.4",
    "tailwindcss": "^3.3.0"
  }
}
